{
  "cli": {
    "version": ">= 7.8.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "enableDangerousExperimentalLeanBuilds": true,
        "androidArchitectures": ["armeabi-v7a"],
        "universalApk": false
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "enableDangerousExperimentalLeanBuilds": true,
        "androidArchitectures": ["armeabi-v7a"],
        "universalApk": false
      }
    },
    "production": {
      "channel": "production",
      "android": {
        "enableDangerousExperimentalLeanBuilds": true,
        "androidArchitectures": ["armeabi-v7a"],
        "universalApk": false
      }
    }
  },
  "submit": {
    "production": {}
  }
} 