{
  "expo": {
    "name": "Music-app",
    "slug": "Music-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "Music-app",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.Music.app",
      "runtimeVersion": "1.0.0",
      "enableDangerousExperimentalLeanBuilds": true,
      "androidArchitectures": [
        "armeabi-v7a"
      ],
      "universalApk": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d8f214c7-38d6-4539-8079-02ce9b0fe8a3"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/d8f214c7-38d6-4539-8079-02ce9b0fe8a3"
    }
  }
}
